package vendor.nameless.hardware.displayfeature@1.0;

interface IDisplayFeature {
    hasFeature(Feature feature) generates (bool rc);
    getFeatureEnabled(Feature feature) generates (bool rc);
    setFeatureEnabled(Feature feature, bool enabled);
    setColorMode(uint32_t mode);
    setDisplayOrientation(uint32_t orientation);
    sendCommand(Command command) generates (int32_t rc);
};
